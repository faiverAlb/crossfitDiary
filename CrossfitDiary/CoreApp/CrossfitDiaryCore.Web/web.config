    <!--         <add input="{HTTPS}" pattern="off" /> -->
    <!--       </conditions> -->
    <!--       <action type="Redirect" url="https://www.cfwod.ru/{R:1}" /> -->
    <!--     </rule> -->
    <!--   </rules> -->
    <!-- </rewrite> -->
  </system.webServer>
</configuration>