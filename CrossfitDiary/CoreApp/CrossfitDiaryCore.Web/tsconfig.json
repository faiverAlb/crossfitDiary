{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "target": "es5",
    "lib": ["es5", "es2015", "dom", "scripthost"],
    "sourceMap": true,
    "skipDefaultLibCheck": true
    ,"strict": false
    //,"types": [ "webpack-env" ]
  },
  "exclude": [
    "bin",
    "node_modules"
  ]
}
