<script type="text/html" id="existing-workouts-template">
    <div class="existing-workouts-container">
        <div class="col-md-12 workout-container">
            <div class="panel panel-success">
                <div class="panel-heading">
                    Existing Workouts
                </div>
                <div class="panel-body">
                    <div class="existing-workout-selector">
                        <select class="form-control" data-live-search="true" data-bind="options: _availableWorkouts, optionsText:'title',value: _selectedWorkout, optionsCaption: 'Choose a workout...',selectPicker: {}"></select>
                    </div>
                    <div data-bind="if: _selectedWorkout">
                        <div data-bind="template: { name: 'routine-info-template', data: {model:_workoutToDisplay} }"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</script>